{
  "name": "nugrpc-monorepo",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -vt run build",
    "test": "yarn workspaces foreach -v run test",
    "test:ci": "yarn build && yarn test",
    "coverage": "yarn workspaces foreach -v run coverage",
    "check": "yarn dlx @yarnpkg/doctor packages/"
  }
}
